{
  "name": "liskit",
  "version": "0.1.0",
  "description": "A simple curated list of all your favourite links",
  "main": "server.js",
  "scripts": {
    "build": "rm -rf dist/ && babel src -d dist",
    "lint": "eslint src",
    "start": "node dist/server.js",
    "serve": "npm run build && dotenv -e envs/.env pm2 start node dist/server.js",
    "test:e2e": "dotenv -e envs/test.env -e envs/.env ./scripts/e2e.test.sh",
    "watch": "nodemon -w src --exec npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Cado-Apps/liskit-api.git"
  },
  "author": "Sherin Binu <sherinbinu@hotmail.com> (https://sherondale.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/Cado-Apps/liskit-api/issues"
  },
  "homepage": "https://gitlab.com/Cado-Apps/liskit-api#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "express": "^4.16.4",
    "mongoose": "^5.4.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "cucumber": "^5.0.3",
    "dotenv-cli": "^1.4.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.2.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "pm2": "^3.2.4",
    "superagent": "^4.0.0"
  }
}
